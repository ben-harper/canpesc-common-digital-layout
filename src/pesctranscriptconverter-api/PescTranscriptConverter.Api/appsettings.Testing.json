{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "GotenbergSharpClient": {
    "ServiceUrl": "http://gotenberg:3000",
    "HealthCheckUrl": "http://gotenberg:3000/health",
    "RetryPolicy": {
      "Enabled": true,
      "RetryCount": 4,
      "BackoffPower": 1.5,
      "LoggingEnabled": true
    }
  },
  "CdlAssets": {
    "RootDirectory": "./cdl" // This is because aspire runs in the project directory, not the bin directory
  }
}
